# CrystaLattE Input Template:

# Blank lines and lines starting with the hash character are omitted.
# Padding blank spaces are also ignored.

cif_input       = Naphthalene.cif
cif_output      = Naphthalene.xyz
cif_a           = 3
cif_b           = 3
cif_c           = 3
nmers_up_to     = 5
r_cut_com       = 9.8
r_cut_monomer   = 4.5
r_cut_dimer     = 2.5
r_cut_trimer    = 2.86
r_cut_tetramer  = 3.6
r_cut_pentamer  = 5.2
cle_run_type    = psi4api + quiet
psi4_method     = HF/STO-3G
psi4_bsse       = cp
psi4_memory     = 500 MB
verbose         = 2
