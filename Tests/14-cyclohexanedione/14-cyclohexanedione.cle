# CrystaLattE Input Template:

# Blank lines and lines starting with the hash character are omitted.
# Padding blank spaces are also ignored.

read_cif_input  = ../Tests/14-cyclohexanedione/14-cyclohexanedione.cif
read_cif_output = ../Tests/14-cyclohexanedione/14-cyclohexanedione.xyz
read_cif_a      = 2
read_cif_b      = 2
read_cif_c      = 2
nmers_up_to     = 3
r_cut_com       = 5.7
r_cut_monomer   = 3.5
r_cut_dimer     = 2.7
r_cut_trimer    = 2.545
r_cut_tetramer  = 2.8
r_cut_pentamer  = 4.9
cle_run_type    = psi4api + quiet
psi4_method     = HF/STO-3G
psi4_bsse       = cp
psi4_memory     = 500 MB
verbose         = 2
